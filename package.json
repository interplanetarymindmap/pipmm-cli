{
  "name": "ipmm-cli",
  "description": "Toolkit to make a FOAM repo to make it Interplanetary",
  "version": "0.0.0",
  "author": "xavivives",
  "bin": {
    "ipmm": "./bin/run"
  },
  "bugs": "https://github.com/xavivives/ipmm-cli/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.16",
    "@types/ini": "^1.3.30",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.58",
    "@types/node-ipc": "^9.1.3",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^9.0.2",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/xavivives/ipmm-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ipmm",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "xavivives/ipmm-cli",
  "scripts": {
    "run": "node bin/run foam import",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@ipld/schema-validation": "^1.1.0",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "cli-ux": "^5.5.1",
    "gray-matter": "^4.0.2",
    "ipfs-core": "^0.10.4",
    "ipld-schema": "^1.1.6",
    "multiformats": "^9.4.5"
  }
}
